{
    "//": "dagger demo configs",
    "options": {
        "//": "dagger options",
        "debug": "location.hostname.includes('dagger.demo.com') || location.hostname.includes('localhost')",
        "commentNode": true,
        "directiveAttribute": true,
        "moduleLog": true,
        "routerLog": true
    },
    "rootSelectors": ["title", "body"],
    "routing": {
        "mode": "history",
        "aliases": {
            "/": "/index"
        },
        "default": "/default",
        "redirects": {
            "/": "/index"
        },
        "scenarios": {
            "//": "path/constants/variables/tailable/modules/children",
            "tailable": true,
            "variables": {},
            "constants": {
                "title": "default, should be overwritten"
            },
            "modules": {
                "test_script": "#root_test_script",
                "script": "#script",
                "component": [{
                    "match": false,
                    "uri": "#component1"
                }, {
                    "match": true,
                    "uri": "#component"
                }],
                "prefix": "#prefix",
                "suffix": "#suffix",
                "directive": {
                    "content": "<div>directive content</div>",
                    "type": "template"
                }
            },
            "children": [{
                "path": "index",
                "constants": {
                    "title": "index",
                    "template": "index"
                },
                "modules": {
                    "@script": {
                        "test_script": "#index_test_script"
                    },
                    "@namespace": {
                        "index": {
                            "content": {
                                "template": [{
                                    "match": false,
                                    "uri": "#index_template123",
                                    "style": "style"
                                }, {
                                    "match": true,
                                    "uri": "#index_template",
                                    "style": "style"
                                }],
                                "style": "#index_style",
                                "content": "./index.json"
                            }
                        }
                    }
                }
            }, {
                "path": "controllers",
                "constants": {
                    "title": "controllers",
                    "template": "controllers"
                },
                "modules": {
                    "controllers": "http://localhost:8283/controllers.html"
                },
                "children": [{
                    "path": "focus",
                    "variables": {
                        "focus": false
                    },
                    "constants": {
                        "title": "focus"
                    },
                    "modules": {
